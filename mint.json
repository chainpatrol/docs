{
  "$schema": "https://mintlify.com/schema.json",
  "name": "ChainPatrol",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://chainpatrol.com"
  },
  "favicon": "/favicon.svg",
  "theme": "venus",
  "font": {
    "headings": {
      "family": "IBM Plex Sans"
    },
    "body": {
      "family": "Inter"
    }
  },
  "colors": {
    "primary": "#6622DC",
    "light": "#985EFD",
    "dark": "#2A186B",
    "background": {
      "dark": "#060114"
    },
    "anchors": {
      "from": "#D922D2",
      "to": "#6622DC"
    }
  },
  "modeToggle": {
    "default": "dark"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "openapi": "https://app.chainpatrol.io/api/v2/openapi.json?v=2",
  "api": {
    "baseUrl": "https://app.chainpatrol.io/api/v2",
    "auth": {
      "method": "key",
      "name": "X-API-KEY"
    }
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://app.chainpatrol.io/"
