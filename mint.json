{
  "$schema": "https://mintlify.com/schema.json",
  "name": "ChainPatrol",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://chainpatrol.io"
  },
  "favicon": "/favicon.svg",
  "theme": "venus",
  "font": {
    "headings": {
      "family": "IBM Plex Sans"
    },
    "body": {
      "family": "Inter"
    }
  },
  "colors": {
    "primary": "#6622DC",
    "light": "#985EFD",
    "dark": "#2A186B",
    "background": {
      "dark": "#060114"
    },
    "anchors": {
      "from": "#D922D2",
      "to": "#6622DC"
    }
  },
  "modeToggle": {
    "default": "dark"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "openapi": "https://app.chainpatrol.io/api/v2/openapi.json",
  "api": {
    "baseUrl": "https://app.chainpatrol.io/api/v2"
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://app.chainpatrol.io/"
  },
  "topbarLinks": [
    {
      "name": "Blog",
      "url": "https://chainpatrol.io/blog"
    },
    {
      "name": "Changelog",
      "url": "https://chainpatrol.io/changelog"
    }
  ],
  "tabs": [
    {
      "name": "API Reference",
      "url": "external-api"
    },
    {
      "name": "JavaScript SDK",
      "url": "sdk"
    },
    {
      "name": "Glossary",
      "url": "glossary"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": ["introduction"]
    },

    {
      "group": "General",
      "pages": [
        "general/how-it-works",
        "general/concepts",
        "general/false-positives"
      ]
    },
    {
      "group": "Integration",
      "pages": [
        "wallet-integration/guide",
        "wallet-integration/check-vs-list",
        "wallet-integration/proxy",
        "wallet-integration/webhooks",
        "wallet-integration/zapier",
        "security-integration/canary-tokens",
        "security-integration/submit-assets-for-scanning"
      ]
    },
    {
      "group": "Bots",
      "pages": ["bots/discord-bot", "bots/slack-bot", "bots/telegram-bot"]
    },
    {
      "group": "External API",
      "pages": ["external-api/overview", "external-api/authentication"]
    },
    {
      "group": "Assets",
      "pages": [
        "external-api/asset-check",
        "external-api/asset-details",
        "external-api/asset-submit",
        "external-api/asset-list",
        "external-api/asset-changelog"
      ]
    },
    {
      "group": "Reports",
      "pages": ["external-api/report-create"]
    },
    {
      "group": "@chainpatrol/sdk",
      "pages": ["sdk/overview", "sdk/quickstart"]
    },
    {
      "group": "Asset Types",
      "pages": [
        "glossary/asset-types/url",
        "glossary/asset-types/page",
        "glossary/asset-types/address"
      ]
    },
    {
      "group": "API Reference",
      "pages": [
        {
          "group": "ThreatDetector",
          "pages": [
            "sdk/threat-detector/overview",
            "sdk/threat-detector/constructor",
            {
              "group": "Instance methods",
              "pages": [
                "sdk/threat-detector/instance-methods/url",
                "sdk/threat-detector/instance-methods/allow",
                "sdk/threat-detector/instance-methods/block",
                "sdk/threat-detector/instance-methods/ignore"
              ]
            }
          ]
        },
        {
          "group": "Storage",
          "pages": [
            "sdk/storage/overview",
            "sdk/storage/browser",
            "sdk/storage/extension",
            "sdk/storage/memory"
          ]
        },
        {
          "group": "Relay",
          "pages": [
            "sdk/relay/overview",
            {
              "group": "Static methods",
              "pages": [
                "sdk/relay/static-methods/send",
                "sdk/relay/static-methods/run",
                "sdk/relay/static-methods/on"
              ]
            }
          ]
        },
        {
          "group": "ChainPatrolClient",
          "pages": [
            "sdk/chainpatrol-client/overview",
            "sdk/chainpatrol-client/constructor",
            {
              "group": "Instance methods",
              "pages": [
                "sdk/chainpatrol-client/instance-methods/asset-check",
                "sdk/chainpatrol-client/instance-methods/asset-list"
              ]
            }
          ]
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/wallet-integration/false-positives",
      "destination": "/general/false-positives"
    },
    {
      "source": "/general/discord-bot",
      "destination": "/bots/discord-bot"
    }
  ],
  "backgroundImage": "/background.png",
  "footerSocials": {
    "twitter": "https://twitter.com/ChainPatrol",
    "linkedin": "https://www.linkedin.com/company/89545915",
    "github": "https://github.com/chainpatrol"
  },
  "analytics": {
    "ga4": {
      "measurementId": "G-QEBBHCD92K"
    }
  }
}
