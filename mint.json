    },
    {
      "source": "/wallet-integration/check-vs-list",
      "destination": "/integration/check-vs-list"
    },
    {
      "source": "/general/discord-bot",
      "destination": "/integration/discord"
    },
    {
      "source": "/bots/discord-bot",
      "destination": "/integration/discord"
    },
    {
      "source": "/bots/telegram-bot",
      "destination": "/integration/telegram"
    },
    {
      "source": "/bots/slack-bot",
      "destination": "/integration/slack"
    },
    {
      "source": "/security-integration/canary-tokens",
      "destination": "/integration/canary-tokens"
    },
    {
      "source": "/api",
      "destination": "/external-api"
    },
    {
      "source": "/api/:slug*",
      "destination": "/external-api/:slug*"
    },
    {
      "source": "/bots/slack",
      "destination": "/integration/slack"
    },
    {
      "source": "/bots/discord",
      "destination": "/integration/discord"
